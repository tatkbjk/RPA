      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable" xml:space="preserve">[(From r In dtItem.AsEnumerable()
 Let stock = If(IsDBNull(r("Stock On Hand")) OrElse r("Stock On Hand").ToString.Trim = "", 0, CInt(r("Stock On Hand")))
 Where stock &lt;= 50
 Let qty = 200 - stock
 Where qty &gt; 0
 Select dtToOrder.LoadDataRow(
     New Object() {
         r("SKU").ToString,
         r("Item Name").ToString,
         r("Preferred Vendor").ToString,
         stock,
         qty
     }, False)
).CopyToDataTable()]</InArgument>
      </Assign.Value>
    </Assign>
